{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "npm",
            "label": "develop",
            "script": "dev",
            "isBackground": true,
            "problemMatcher": [
                {
                    "owner": "javascript",
                    "pattern": [
                        {
                            "regexp": "^âœ– (ERROR|WARNING) ([^\\s].*)$",
                            "severity": 1,
                            "file": 2
                        },
                        {
                            "regexp": "^([\\s.]* \\((\\d+):(\\d+)\\)+)",
                            "message": 1,
                            "line": 2,
                            "column": 3
                        },
                        {
                            "regexp": "^ @ ([^\\s].*) ((\\d+):((\\d+)(\\-(\\d+))?) ?)*$",
                            "file": 1,
                            "line": 3,
                            "column": 5,
                            "endColumn": 7,
                            "loop": true
                        }
                    ],
                    "background": {
                        "activeOnStart": true,
                        "beginsPattern": "[.\\[\\].]*Compiled successfully!$",
                        "endsPattern": "^On Your Network:  https?://(?:\\d{1,3}\\.){3}\\d{1,3}:\\d+$"
                    }
                }
            ]
        }
    ]
}